Que 12)#include<iostream>
using namespace std;

string revString ( string str ){
    int i = 0;
    int j = str.length()- 1;
    string ans ;
    while(i <= j ){
        swap(str[i] , str[j]);
        i++;
        j--;
    }
    
    ans = str ;
    return ans;
    
}

int main (){
    string str;
    cout<<" Enter a string :" ;
    cin>> str ;
    string revStr = revString(str);
    cout<<revStr;
    return 0;
}


Que 14)#include<iostream> 
using namespace std; 

bool palindrome(string str){
    int s= 0; int e= str.length()-1; 
    while(s<e){
        if(str[s] != str[e]){
            return false; 
        }
        s++; 
        e--; 
    }

    return true; 
}

int main(){

    string input; 

    cout<<"Enter the input string: "; 
    cin>>input; 

    if(palindrome(input)){
        cout<<"yes! The given string is palindrome"<<endl;
    }else{
        cout<<"No! The given string is not a palindrome"<<endl;
    }


    return 0; 

}

Que 15)public class countOcc {
    public static void main(String[] args) {
    String input=" Welcome to SunBeam";
   String unique=" ";
String x[]=input.split("");
for String s:x
{
if(unique.contains(s)==false)
{
unique=unique+s;
}
} 
system.out.println(unique);
string y[]=unique.split("");
for(String a:y)
{
int counter=0;
for(String b:x)
{
if(a.equals(b))
{
counter ++;
}
}

} 
